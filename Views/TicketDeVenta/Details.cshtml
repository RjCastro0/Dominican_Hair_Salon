@model Dominican_Hair_Salon.Models.TicketDeVenta

@{
    ViewData["Title"] = "Detalles";
}

<h1>Detalles</h1>

<div>
    <h4>Ticket De Venta</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Estilista
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Empleada)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Precio)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Precio)
        </dd>
        <dt class = "col-sm-2">
            Cliente Nombre
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ClienteNombre)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Surcursal)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Surcursal.Nombre)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Fecha)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Fecha)
        </dd>

    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.TicketId">Editar</a> |
    <a asp-action="Index">Volver</a>
</div>
